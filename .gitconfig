[user]
	name = Dan Suh
	email = kehops@kaist.ac.kr
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[push]
	default = simple
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process

